{
  "name": "ec-pwa",
  "version": "1.0.0",
  "main": "server.js",
  "description": "React . Apollo (GraphQL) . Next.js Toolkit",
  "author": "Ilker Guller <me@ilkerguller.com> (http://ilkerguller.com)",
  "repository": {
    "type": "git",
    "url": ""
  },
  "bugs": {
    "url": ""
  },
  "keywords": [],
  "engines": {
    "node": ">=8.9.0",
    "npm": ">=4"
  },
  "scripts": {
    "flow": "flow",
    "update-types": "flow-typed update",
    "start": "cross-env NODE_ENV=production node server",
    "build": "npm run clean-cache && modclean -r && next build",
    "clean-cache": "rimraf -rf ./node_modules/.cache",
    "dev": "npm run lint && npm run clean-cache && nodemon server.js",
    "dev:debug": "npm run lint && npm run clean-cache && nodemon --inspect-brk server.js",
    "dev:next": "npm run lint && npm run clean-cache && next",
    "lint": "npm run lint:es && npm run lint:flow",
    "lint:es": "eslint --fix --ext .js pages components data graphql store theme utils server",
    "lint:flow": "flow check --include pages,components,data,graphql,store,theme,utils",
    "lint:watch": "esw -w --fix components libraries server",
    "analyze": "cross-env ANALYZE_BUILD=true npm run build",
    "build:static_export": "cross-env STATIC_EXPORT=true npm run build && cross-env STATIC_EXPORT=true next export"
  },
  "dependencies": {
    "apollo-client": "1.9.3",
    "apollo-link": "1.0.6",
    "apollo-link-http": "1.3.1",
    "apollo-server-express": "1.3.0",
    "autosuggest-highlight": "3.1.1",
    "babel-plugin-inline-dotenv": "1.1.1",
    "babel-plugin-inline-import-graphql-ast": "2.0.4",
    "babel-plugin-transform-flow-strip-types": "6.22.0",
    "babel-plugin-transform-inline-environment-variables": "0.2.0",
    "babel-preset-stage-2": "6.24.1",
    "chalk": "2.3.0",
    "classnames": "2.2.5",
    "compression": "1.7.1",
    "cross-env": "5.1.1",
    "debounce": "1.1.0",
    "dotenv": "4.0.0",
    "express": "4.16.2",
    "graphql": "0.11.7",
    "graphql-tag": "2.6.0",
    "graphql-tools": "2.13.0",
    "helmet": "3.9.0",
    "immutable": "4.0.0-rc.9",
    "ip": "1.1.5",
    "isomorphic-fetch": "2.2.1",
    "js-cookie": "2.2.0",
    "jss-nested": "6.0.1",
    "lru-cache": "4.1.1",
    "material-ui": "1.0.0-beta.23",
    "material-ui-icons": "1.0.0-beta.17",
    "moment": "2.19.4",
    "morgan": "1.9.0",
    "next": "4.2.3",
    "next-routes": "1.2.0",
    "offline-plugin": "4.8.5",
    "prop-types": "15.6.0",
    "react": "16.2.0",
    "react-apollo": "2.0.4",
    "react-autosuggest": "9.3.2",
    "react-dom": "16.2.0",
    "react-helmet": "5.2.0",
    "react-redux": "5.0.6",
    "recompose": "0.26.0",
    "redux": "3.7.2",
    "redux-immutable": "4.0.0",
    "redux-logger": "3.0.6",
    "rimraf": "2.6.2",
    "styled-jss": "2.1.2"
  },
  "devDependencies": {
    "babel-eslint": "8.0.3",
    "babel-plugin-transform-assets": "0.2.0",
    "babel-plugin-transform-export-extensions": "6.22.0",
    "eslint": "4.13.1",
    "eslint-config-airbnb": "16.1.0",
    "eslint-config-fravega": "1.5.0",
    "eslint-config-prettier": "2.9.0",
    "eslint-loader": "1.9.0",
    "eslint-plugin-flowtype": "2.40.1",
    "eslint-plugin-graphql": "1.4.1",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-jsx-a11y": "6.0.3",
    "eslint-plugin-prettier": "2.3.1",
    "eslint-plugin-react": "7.5.1",
    "eslint-watch": "3.1.3",
    "flow-bin": "0.61.0",
    "flow-typed": "2.2.3",
    "modclean": "2.1.2",
    "nodemon": "1.12.7",
    "webpack-bundle-analyzer": "2.9.1"
  }
}
